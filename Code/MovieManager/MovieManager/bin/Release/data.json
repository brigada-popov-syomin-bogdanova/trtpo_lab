0442\u0435\u0440\u0435\u0441\u043D\u0430\u044F \u0447\u0430\u0441\u0442\u044C \u043F\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u044F \u2013 \u0435\u0433\u043E \u0414\u043E\u0440\u043E\u0433\u0430 \u0421\u043C\u0435\u0440\u0442\u0438, \u043A\u043E\u0442\u043E\u0440\u0430\u044F \u0442\u044F\u043D\u0435\u0442\u0441\u044F \u043D\u0430 \u043F\u0440\u043E\u0442\u044F\u0436\u0435\u043D\u0438\u0438 \u0432\u0441\u0435\u0439 \u0417\u0435\u043B\u0435\u043D\u043E\u0439 \u041C\u0438\u043B\u0438. \u041F\u043E\u0441\u043B\u0435 \u0442\u043E\u0433\u043E \u043A\u0430\u043A \u043F\u0440\u0438\u0433\u043E\u0432\u043E\u0440 \u0431\u044B\u043B \u043F\u0440\u0438\u0432\u0435\u0434\u0435\u043D \u0432 \u0438\u0441\u043F\u043E\u043B\u043D\u0435\u043D\u0438\u0435 \u0432\u044B\u044F\u0441\u043D\u0438\u043B\u043E\u0441\u044C, \u0447\u0442\u043E \u0443 \u0414\u0436\u043E\u043D\u0430 \u041A\u043E\u0444\u0444\u0438 \u0438\u043C\u0435\u0435\u0442\u0441\u044F \u0435\u0449\u0435 \u043E\u0434\u043D\u0430 \u043E\u0441\u043E\u0431\u0435\u043D\u043D\u043E\u0441\u0442\u044C, \u0430 \u0438\u043C\u0435\u043D\u043D\u043E \u0441\u043F\u043E\u0441\u043E\u0431\u043D\u043E\u0441\u0442\u044C \u0438\u0437\u043B\u0435\u0447\u0438\u0432\u0430\u0442\u044C. \u041E\u043D \u043F\u043E\u043C\u043E\u0433\u0430\u0435\u0442 \u0433\u0435\u0440\u043E\u044F\u043C \u0438\u0437\u0431\u0435\u0436\u0430\u0442\u044C \u0442\u044F\u0436\u0435\u043B\u044B\u0445 \u0431\u043E\u043B\u0435\u0437\u043D\u0435\u0439, \u0430 \u043C\u0430\u043B\u0435\u043D\u044C\u043A\u0443\u044E \u043C\u044B\u0448\u043A\u0443 \u041C\u0438\u0441\u0442\u0435\u0440\u0430 \u0414\u0436\u0438\u043D\u0433\u043B\u0441\u0430 \u0438 \u0432\u043E\u043E\u0431\u0449\u0435 \u0432\u043E\u0437\u0432\u0440\u0430\u0449\u0430\u0435\u0442 \u043A \u0436\u0438\u0437\u043D\u0438. \u0418 \u0445\u043E\u0442\u044F \u0435\u0433\u043E \u0434\u0430\u0440 \u043D\u0435 \u0432\u0441\u0435\u0433\u0434\u0430 \u0431\u044B\u0432\u0430\u0435\u0442 \u043B\u0435\u0433\u043A\u0438\u043C, \u043D\u043E \u043E\u043D \u0432\u0441\u0435 \u0440\u0430\u0432\u043D\u043E \u0441\u0442\u0440\u0430\u0434\u0430\u0435\u0442 \u043E\u0442 \u0431\u043E\u043B\u0438, \u0436\u0435\u0441\u0442\u043E\u043A\u043E\u0441\u0442\u0438 \u0438 \u043D\u0435\u043D\u0430\u0432\u0438\u0441\u0442\u0438, \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043F\u0435\u0440\u0435\u043F\u043E\u043B\u043D\u044F\u044E\u0442 \u0435\u0433\u043E \u043C\u0438\u0440. \u0413\u043B\u0430\u0432\u043D\u044B\u0439 \u0442\u044E\u0440\u0435\u043C\u0449\u0438\u043A \u0431\u043B\u043E\u043A\u0430 \u0441\u043C\u0435\u0440\u0442\u043D\u0438\u043A\u043E\u0432, \u041F\u043E\u043B \u042D\u043A\u0434\u0436\u043A\u043E\u043C\u0431, \u0441 \u0441\u0430\u043C\u043E\u0433\u043E \u043D\u0430\u0447\u0430\u043B\u0430 \u043F\u043E\u043D\u0438\u043C\u0430\u043B, \u0447\u0442\u043E \u0441\u043A\u043E\u0440\u0435\u0435 \u0432\u0441\u0435\u0433\u043E \u041A\u043E\u0444\u0444\u0438 \u043D\u0435 \u0432\u0438\u043D\u043E\u0432\u0430\u0442, \u0430 \u043F\u043E\u0442\u043E\u043C \u0438 \u0443\u0437\u043D\u0430\u0435\u0442 \u0432\u0441\u044E \u043F\u0440\u0430\u0432\u0434\u0443 \u0431\u0435\u0437 \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u0438 \u0434\u043E\u043A\u0430\u0437\u0430\u0442\u044C \u0435\u0435 \u043D\u0435\u0432\u0438\u043D\u043E\u0432\u043D\u043E\u0441\u0442\u044C. \u041F\u043E \u0441\u043B\u043E\u0432\u0430\u043C \u0441\u0430\u043C\u043E\u0433\u043E \u0433\u0435\u0440\u043E\u044F, \u043E\u043D \u043E\u0447\u0435\u043D\u044C \u0441\u0438\u043B\u044C\u043D\u043E \u043F\u0435\u0440\u0435\u0436\u0438\u0432\u0430\u0435\u0442 \u0432\u0441\u0435 \u0447\u0443\u0432\u0441\u0442\u0432\u0430 \u0432\u0435\u043B\u0438\u043A\u0430\u043D\u0430 \u043D\u0430 \u0414\u043E\u0440\u043E\u0433\u0438 \u0421\u043C\u0435\u0440\u0442\u0438 \u0438 \u0441\u0442\u0430\u0440\u0430\u0435\u0442\u0441\u044F \u0438\u043C \u043A\u0430\u043A-\u0442\u043E \u043F\u043E\u0441\u043E\u0434\u0435\u0439\u0441\u0442\u0432\u043E\u0432\u0430\u